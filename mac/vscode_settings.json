{
  /**
   ** --- General Editor Settings ----
   **/
  "workbench.tree.indent": 10,
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": false,
  "editor.fontSize": 12,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  // "editor.acceptSuggestionOnEnter": "off",
  // "editor.fontFamily": "Cascadia Code PL",
  "editor.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",
  // "editor.fontFamily": "Hasklig",
  // "editor.fontFamily": "Hack Nerd Font",
  "editor.fontWeight": "350",
  "editor.fontLigatures": true,
  "window.zoomLevel": 0,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "explorer.confirmDelete": false,
  "workbench.editor.showTabs": true,
  "workbench.list.automaticKeyboardNavigation": false,
  "editor.minimap.enabled": true,
  "workbench.activityBar.visible": true,
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.colorTheme": "Atom One Dark",
  // "oneDarkPro.editorTheme": "oneDarkProFlat",
  "window.menuBarVisibility": "toggle",
  "terminal.integrated.shell.linux": "/bin/zsh",
  // "editor.tokenColorCustomizations": {
  //   "[Horizon]": {
  //     "comments": "#838383"
  //   }
  // },
  "material-icon-theme.folders.associations": {
    "actions": "Redux-actions",
    "reducers": "Redux-reducer"
  },
  "explorer.confirmDragAndDrop": false,
  "editor.semanticHighlighting.enabled": true,
  // "editor.semanticTokenColorCustomizations": {
  //   "[Darcula Theme from IntelliJ]": {
  //     "rules": {
  //       "variable.readonly": "#A9B7C6"
  //     }
  //   }
  // },
  /**
   ** ---- Terminal Settings ----
   **/
  "workbench.colorCustomizations": {
    // The background is optional
    "terminal.background": "#323336",
    "terminalCursor.background": "#282a36",
    "terminalCursor.foreground": "#97979b",
    "terminal.selectionBackground": "#97979b33",
    "terminal.foreground": "#eff0eb",
    "terminal.ansiBlack": "#282a36",
    "terminal.ansiRed": "#ff5c57",
    "terminal.ansiGreen": "#5af78e",
    "terminal.ansiYellow": "#ff950a",
    // "terminal.ansiBlue": "#57c7ff",
    "terminal.ansiBlue": "#0084ff",
    "terminal.ansiMagenta": "#ff6ac1",
    "terminal.ansiCyan": "#9aedfe",
    "terminal.ansiWhite": "#f1f1f0",
    "terminal.ansiBrightBlack": "#686868",
    "terminal.ansiBrightRed": "#ff5c57",
    "terminal.ansiBrightGreen": "#5af78e",
    "terminal.ansiBrightYellow": "#f3f99d",
    "terminal.ansiBrightBlue": "#57c7ff",
    "terminal.ansiBrightMagenta": "#ff6ac1",
    "terminal.ansiBrightCyan": "#9aedfe",
    "terminal.ansiBrightWhite": "#eff0eb",
    // "editorBracketMatch.background": "#acacac9f",
    // "editorBracketMatch.border": "#ffffff",
    "editor.selectionBackground": "#5aa9dd7c", //Current selected text
    "editor.selectionHighlightBackground": "#ff00005b", //Same content as selection
    //"editor.findMatchBackground": "#00cc44a8", //Current SEARCH MATCH
    "editor.findMatchHighlightBackground": "#ad5400a1", //Other SEARCH MATCHES
    "[One Dark Pro]": {
      "editor.background": "#1e2127"
    },
    "[Atom One Dark]": {
      "editor.background": "#1e2127"
    },
    "[Horizon Italic]": {
      "editor.background": "#1f2125"
    }
  },
  "terminal.external.osxExec": "iterm2",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "terminal.integrated.fontSize": 13,
  "vsicons.dontShowNewVersionMessage": true,
  /**
   ** ---- Python Settings ----
   **/
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": [
    "--ignore=E203",
    "--ignore=E266",
    "--ignore=E501",
    "--ignore=W503",
    "--max-line-length=100",
    "--select = B,C,E,F,W,T4,B9",
    "--max-complexityj = 18"
  ],
  "python.languageServer": "Microsoft",
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["--line-length", "120"],
  "code-runner.executorMap": {
    "python": "$pythonPath -u $fullFileName"
  },
  /**
   ** ---- C/CPP Settings ----
   **/
  "C_Cpp.clang_format_fallbackStyle": "Mozilla",
  "C_Cpp.default.cStandard": "c99",
  "C_Cpp.default.cppStandard": "c++11",
  // "C_Cpp.default.intelliSenseMode": "gcc-x64",
  "C_Cpp.clang_format_style": "{BasedOnStyle: Google, ColumnLimit: 0, UseTab: Never, IndentWidth: 4, TabWidth: 4}",
  "code-runner.showExecutionMessage": false,
  "code-runner.clearPreviousOutput": true,
  "python.linting.enabled": true,
  "files.associations": {
    "*.gpl": "cpp",
    "*.js": "javascriptreact",
    "*.rmd": "markdown",
    "*.Rmd": "rmd"
  },
  "C_Cpp.updateChannel": "Insiders",
  /**
   ** ---- Javascript Settings ----
   **/
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "prettier.singleQuote": true,
  /**
  /**
  ** ---- R Settings ----
  **/
  "r.sessionWatcher": true,
  "r.bracketedPaste": true,
  "r.linting.enable": true,
  "r.rterm.mac": "/Library/Frameworks/Python.framework/Versions/3.8/bin/radian",

  /** ---- Better Comments tag colors ----
   **/
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "//",
      "color": "#81818181",
      "strikethrough": true,
      "backgroundColor": "transparent"
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "backgroundColor": "transparent"
    }
  ],
  /**
   ** ---- Vim/Neovim Settings ----
   **/
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/local/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "$HOME/.config/nvim/init.vim",
  "vscode-neovim.neovimInitVimPaths.linux": "$HOME/.config/nvim/init.vim",
  /**
   ** ---- Dart / Flutter Settings ----
   **/
  "dart.runPubGetOnPubspecChanges": false,
  "dart.flutterCreateOrganization": "com.",
  "dart.previewFlutterUiGuides": true,
  "dart.flutterSdkPath": "/Users/bmarden/development/flutter",
  "[dart]": {
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  /**
   ** ---- Markdown Settings ----
   */
  "markdownlint.config": {
    "MD013": false,
    "MD033": false
  },
  "markdown.styles": [
    "https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
    // "https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css",
  ],
  "markdown.extension.math.enabled": false,
  "markdown-preview-enhanced.previewTheme": "atom-material.css",
  /**
   ** ---- LaTeX Settings ----
   **/
  "latex-workshop.latex.autoBuild.run": "onFileChange",
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.outDir": "out.nosync",
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "--shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ]
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "--shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-output-directory=out.nosync",
        "-aux-directory=out.nosync",
        "%DOC%"
      ]
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": ["out.nosync/%DOCFILE%"]
    }
  ],
  "[latex]": {
    "editor.formatOnSave": false
  },
  "settingsSync.ignoredSettings": ["vscode-neovim.neovimInitPath"],
  /**
   ** ---- Gitlens Settings ----
   **/
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.lineHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "gitlens.mode.statusBar.alignment": "left",
  "gitlens.statusBar.alignment": "left",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": true,
      "compare": true,
      "details": false,
      "history": false,
      "remote": false
    },
    "editorGroup": false,
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true,
      "stashInline": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true,
      "stash": true
    }
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
