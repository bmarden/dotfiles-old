{
    "workbench.settings.useSplitJSON": true,
    "workbench.settings.editor": "json",
    "editor.fontLigatures": true,
    "editor.fontFamily": "FiraCode Nerd Font, Retina",
    "editor.fontSize": 13,
    "terminal.integrated.fontFamily": "DroidSansMono Nerd Font, Book",
    "terminal.integrated.rendererType": "canvas",
    "terminal.integrated.fontSize": 13,
    "editor.minimap.maxColumn": 120,
    "editor.wordWrap": "off",
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.wordWrapColumn": 80,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": false
        }
    },
    "editor.formatOnSave": true,
    // "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "C_Cpp.clang_format_fallbackStyle": "Visual Studio",
    // Python
    "code-runner.executorMap": {
        "python": "$pythonPath -u $fullFileName",
    },
    "code-runner.showExecutionMessage": false,
    "code-runner.clearPreviousOutput": true,
    // Flutter
    // Vim 
    "vscode-neovim.neovimInitPath": "/home/bmarden/.config/nvim/init.vim",
    "vscode-neovim.neovimPath": "/snap/bin/nvim",
    // "workbench.colorCustomizations": {
    //     "statusBar.background": "#005f5f",
    //     "statusBar.noFolderBackground": "#005f5f",
    //     "statusBar.debuggingBackground": "#005f5f"
    // },
    "C_Cpp.default.cStandard": "c99",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "editor.multiCursorModifier": "ctrlCmd",
    "terminal.integrated.shell.linux": "/usr/bin/zsh",
    "C_Cpp.clang_format_style": "{BasedOnStyle: Google, UseTab: Never, IndentWidth: 4, TabWidth: 4, ColumnLimit: 0}",
    "workbench.iconTheme": "eq-material-theme-icons-ocean",
    "editor.acceptSuggestionOnEnter": "off",
    "gitlens.views.repositories.location": "gitlens",
    "gitlens.views.fileHistory.location": "gitlens",
    "gitlens.views.lineHistory.location": "gitlens",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    // Dart/flutter stuff
    "dart.previewFlutterUiGuides": true,
    "dart.autoImportCompletions": true,
    "dart.runPubGetOnPubspecChanges": false,
    "debug.console.fontSize": 13,
    "workbench.colorTheme": "Monokai Pro (Filter Ristretto)",
    // "[dart]": {
    //     "editor.rulers": [
    //         80
    //     ],
    // },
    // "dart.lineLength": 100
}